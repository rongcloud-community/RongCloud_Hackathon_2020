<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/chatroom_user_item_main"
    android:layout_width="76dp"
    android:layout_height="76dp">

    <RelativeLayout
        android:id="@+id/chatroom_user_item_rel"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:visibility="gone">

        <RelativeLayout
            android:id="@+id/chatroom_user_item_bg"
            android:layout_width="76dp"
            android:layout_height="76dp"
            android:visibility="gone">

            <View
                android:id="@+id/chatroom_user_item_bgexternal"
                android:layout_width="76dp"
                android:layout_height="76dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/bg_room_item" />

            <View
                android:id="@+id/chatroom_user_item_bginside"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/bg_room_item_center" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/chatroom_user_item_imgwheet"
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/chatroom_user_item_img"
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                tools:src="@mipmap/ic_launcher" />

            <ImageView
                android:id="@+id/chatroom_user_item_lockwheet"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_margin="1dp"
                android:src="@mipmap/chatroom_mute"
                android:visibility="gone" />

        </RelativeLayout>


    </RelativeLayout>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/chatroom_user_item_lock"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@mipmap/chatroom_ban"
        android:visibility="gone" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/chatroom_user_item_add"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@mipmap/chatroom_joinroom"
        android:visibility="visible" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/chatroom_user_item_null"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@mipmap/ic_chatroom_nullname"
        android:visibility="gone" />

</RelativeLayout>