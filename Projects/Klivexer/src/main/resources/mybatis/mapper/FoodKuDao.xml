<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.aihome.aihomesys.dao.IFoodKuDao">

    <select id="selectById" resultType="FoodKu">
        SELECT * FROM FOOD_KU WHERE foodId = #{foodId};
    </select>

    <select id="selectFoodListByHotel" resultType="FoodKu">
        SELECT * FROM FOOD_KU WHERE hotelId = #{hotelId};
    </select>

</mapper>